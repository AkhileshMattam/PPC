{
	"name": "ppc",
	"displayName": "PPC(Python Project Creator)",
	"description": "A extension which creates basic files for a python project",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ppc.createPythonProject"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ppc.createPythonProject",
				"title": "Create Python Project"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"author": {
		"name": "Abhishek Kumar"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/iamAbhishekkumar/PPC"
	},
	"publisher": "PPC-Abhishek"
}